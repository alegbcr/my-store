{
  "rewrites": [
    {
      "source": "/",
      "destination": "/api"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api"
    }
  ],

  "routes": [
    {
      "source": "/styles.css",
      "destination": "/public/styles.css"
    }
  ],

  "builds": [
    {
      "src": "/api/index.js",
      "use": "@now/node"
    }
  ]
}
